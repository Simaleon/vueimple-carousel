{
  "name": "@vueimple/carousel",
  "version": "1.1.11",
  "type": "module",
  "author": "Simaleon (https://github.com/simaleon)",
  "description": "Simple Vue 3 carousel",
  "keywords": [
    "Vue",
    "Vue 3",
    "Carousel"
  ],
  "files": ["dist/*", "types/*"],
  "types": "types/index.d.ts",
  "main": "dist/carousel.umd.cjs",
  "module": "dist/carousel.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/carousel.js",
      "require": "./dist/carousel.umd.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simaleon/vueimple-carousel.git"
  },
  "scripts": {
    "dev": "vite demo -c ./vite.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "lint:script": "eslint . --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepare": "vite build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.6.0",
    "jsdom": "^20.0.3",
    "less": "^4.1.3",
    "postcss": "^8.4.18",
    "postcss-less": "^6.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.4",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.9",
    "vue": "^3.2.47"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
